<Project Sdk="Microsoft.Build.NoTargets" InitialTargets="WriteStuff">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SharedFilesSdk</PackageId>
    <Description>Shared files SDK</Description>
  </PropertyGroup>
  <Import Project="./Sdk/Sdk.props" />
  <Target Name="WriteStuff">
    <Message Text="SharedFolder: @(SharedFolder)" />
    <Message Text="SharedFile: @(SharedFile)" />
  </Target>
</Project>
