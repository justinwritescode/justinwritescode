		<PackageReference Include="Microsoft.CodeAnalysis" />
		<PackageReference Include="Microsoft.Azure.Webjobs.Script.ExtensionsMetadataGenerator" />
		<PackageReference Include="Microsoft.Azure.WebJobs.Extensions.Http" />
		<PackageReference Include="System.Net.Mime.MediaTypeNames" ExcludeAssets="ContentFiles"/>
		<PackageReference Include="Microsoft.Extensions.Logging" VersionOverride="[6.0.0,)" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" VersionOverride="[6.0.0,)" />
		<PackageReference Include="Microsoft.Extensions.Logging.Configuration" VersionOverride="[6.0.0,)" />
		<PackageReference Include="Microsoft.Extensions.Configuration" VersionOverride="[6.0.0,)" />
		<PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" VersionOverride="[6.0.0,)" />
  </ItemGroup>
	<ItemGroup>
		<Using Include="System.Net.Mime.MediaTypeNames" Static="true" />
		<Using Include="JustinWritesCode.Functions.Strings.Constants.Headers" Static="true" />
		<Using Include="JustinWritesCode.Functions.Strings.Constants" Static="true" />
		<Using Include="JustinWritesCode.Functions.Strings.Constants.Routes" Static="true" />
		<Using Include="JustinWritesCode.Functions.Strings.Constants.Tags" Static="true" />
	</ItemGroup>
