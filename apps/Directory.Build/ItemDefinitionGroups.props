<Project>
  <ItemDefinitionGroup>
      <Using Visible="false" />
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <PackageReference>
    	<!-- Generate "PKG_Package_Id" properties, which point to the package's restored location on disk -->
			<GeneratePathProperty>true</GeneratePathProperty>
			<IncludeAssets>Compile; Runime; Build; BuildMultitargeting; BuildTransitive; Analyzers; Native</IncludeAssets>
			<ExcludeAssets>ContentFiles</ExcludeAssets>
		</PackageReference>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <ProjectReference>
			<SetConfiguration>Configuration=$(Configuration)</SetConfiguration>
		</ProjectReference>
  </ItemDefinitionGroup>
</Project>
