<Project>
  <ItemDefinitionGroup>
      <Using Visible="false" />
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <!-- Generate "PKG_Package_Id" properties, which point to the package's restored location on disk -->
    <PackageReference GeneratePathProperty="true" IncludeAssets="Compile; Runime; Build; BuildMultitargeting; BuildTransitive; Analyzers; Native" ExcludeAssets="ContentFiles" />
      <!-- <Condition>!@(PackageReference->AnyHaveMetadataValue('Identity', %(Identity))) And '%(Update)' == ''</Condition> -->
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <!-- Generate "PKG_Package_Id" properties, which point to the package's restored location on disk -->
    <ProjectReference SetConfiguration="Configuration=$(Configuration)"  />
  </ItemDefinitionGroup>
</Project>
